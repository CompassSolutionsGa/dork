{
    "manifest_version": 3,
    "name": "Google Dork Backlink Finder",
    "version": "1.0",
    "description": "Finds all backlinks to a given domain using Google Dorking.",
    "permissions": ["tabs", "storage"],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
      }
    }
}
